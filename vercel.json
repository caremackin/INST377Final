{
    "version": 2,
    "builds": [
      { "src": "server/index.js", "use": "@vercel/node" },
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "styling.css", "use": "@vercel/static" },
      { "src": "assets/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/", "dest": "/home.html" },
      { "src": "/(home|main|about)\\.html", "dest": "/$1.html" },
      { "src": "/(.*)", "dest": "/server/index.js" }
    ]
  }
  